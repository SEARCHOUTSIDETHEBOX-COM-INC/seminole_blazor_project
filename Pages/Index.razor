@page "/"

<PageTitle>Home</PageTitle>

<!--Top Info Section Begin-->
<div class="text-center top-info">
    HOMESTEAD EXEMPTION DEADLINE IS MARCH 1.2023 | <a href="#"><em>CLICK HERE TO FILE ONLINE ></em></a>
</div>
<!--Top Info Section Begin-->

<!--Banner Section Begin-->
<div class="home-banner row">
    <img src="../images/home-banner.jpg" />
    <div class="container banner-content">
        <div class="col-12 col-lg-9">
            <h2>Property Search</h2>
            <form class="home-form">
                <div class="form-row">
                    <div class="form-group col-md-12">
                        <select id="inputState" class="form-control">
                            <option selected>Real Property</option>
                            <option>Real Property 1</option>
                            <option>Real Property 2</option>
                            <option>Real Property 3</option>
                            <option>Real Property 4</option>
                        </select>
                    </div>
                </div>
                <div class="form-row">
                    <div class="form-group col-md-12">
                        <input type="text" class="form-control" id="inputSearch" placeholder="Search By Owner (Last Name, First Name)">
                    </div>
                </div>
                <div class="form-row column-4">
                    <div class="form-group col-md-4 streetNo">
                        <input type="text" class="form-control" id="inputStreetNo" placeholder="Street No.">
                    </div>
                    <div class="form-group col-md-2">
                        <select id="inputDir" class="form-control">
                            <option selected>Dir</option>
                            <option>Dir 1</option>
                            <option>Dir 2</option>
                            <option>Dir 3</option>
                        </select>
                    </div>
                    <div class="form-group col-md-4 streetName">
                        <input type="text" class="form-control" id="inputStreetName" placeholder="Street Name">
                    </div>
                    <div class="form-group col-md-2">
                        <select id="inputSuffix" class="form-control">
                            <option selected>Suffix</option>
                            <option>Suffix 1</option>
                            <option>Suffix 2</option>
                            <option>Suffix 3</option>
                        </select>
                    </div>
                </div>

                <div class="form-row">
                    <input type="text" class="form-control" id="inputParcelNo" placeholder="Enter Parcel No. With OR Without Dashes">
                </div>

                <div class="form-row">
                    <input type="text" class="form-control" id="inputSearchByFacility" placeholder="Search By Facility">
                </div>

                <div class="form-row">
                    <input type="text" class="form-control" id="inputSearchBySub" placeholder="Search By Subdivision">
                </div>

                <div class="form-row">
                    <div class="form-check">
                        <input class="form-check-input" type="checkbox" id="gridCheck">
                        <label class="form-check-label" for="gridCheck">
                            On execute, I accept the Terms of Use
                        </label>
                    </div>
                </div>
                <button type="submit" class="btn btn-primary">Submit</button>
            </form>
        </div>
    </div>
</div>
<!--Banner Section End-->
<!--Home Tabs Section Begin-->
<div class="home-tabs">
    <div class="row">
        <div class="col-6 col-lg-3 col-sm-6">
            <ul class="nav tabs-left sideways">
                <li class="active"><a href="#home-v" data-toggle="tab">ANNOUNCEMENTS</a></li>
                <li class=""><a href="#profile-v" data-toggle="tab">IMPORTANT DATES</a></li>
                <li class="third"><a href="#messages-v" data-toggle="tab">MISCELLANEOUS INFORMATION</a></li>
                <li class=""><a href="#settings-v" data-toggle="tab">QUICK LINKS</a></li>
            </ul>
        </div>
        <div class="col-6 col-lg-9 col-sm-6">
            <div class="tab-content">
                <div class="tab-pane active" id="home-v">
                    <ul>
                        <li><span>></span> Job Opening: Public Service Specialist</li>
                        <li><span>></span> Report Hurricane lan property damage online</li>
                        <li><span>></span> Hurricane lan Post Storm Resources</li>
