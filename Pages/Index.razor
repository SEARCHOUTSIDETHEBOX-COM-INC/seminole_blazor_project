<!--Location Form Section Begin-->
<div class="contact-form">
    <div class="row">
        <div class="col-12 col-lg-6 col-md-6 left">
            <h3>HAVE QUESTIONS ?<br />CONTACT US!</h3>
            <p>Please leave us your details and questions in the form below and we will reply shortly.</p>
            <div class="row justify-content-left">
                <div class="col-md-8">
                    <span class="anchor" id="formContact"></span>
                    <form autocomplete="off" class="form" role="form">
                        <fieldset>
                            <div class="row mb-1">
                                <div class="col-lg-12">
                                    <input class="form-control" id="name2" name="name2" required="" type="text" placeholder="First and Last Name">
                                </div>
                            </div>
                            <div class="row mb-1">
                                <div class="col-lg-12">
                                    <input class="form-control" id="phone" name="phone" required="" type="text" placeholder="Phone Number">
                                </div>
                            </div>
                            <div class="row mb-1">
                                <div class="col-lg-12">
                                    <input class="form-control" id="email2" name="email2" required="" type="text" placeholder="Email">
                                </div>
                            </div>
                            <div class="row mb-1">
                                <div class="col-lg-12">
                                    <textarea class="form-control" id="message2" name="message2" required="" rows="6" placeholder="Message"></textarea>
                                </div>
                            </div>
                            <button class="btn btn-secondary btn-lg float-right" type="submit">Submit</button>
                        </fieldset>
                    </form>
                </div><!--/col-->
            </div>
        </div>

        <div class="col-12 col-lg-6 col-md-6 right">
